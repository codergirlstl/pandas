# import module

import pandas as pd

 

# Read excel file

# and store into a DataFrame

df1 = pd.read_excel('CSGrepout.xlsx')

df2 = pd.read_excel('ICMrepout.xlsx')

df3 = pd.read_excel('CSGsuperout.xlsx')

df4 = pd.read_excel('ICMsuperout.xlsx')

df5 = pd.read_excel('CSGmgrout.xlsx')

df6 = pd.read_excel('ICMmgrout.xlsx')

df7 = pd.read_excel('CSGOVNrepout.xlsx')

df8 = pd.read_excel('ICMOVNrepout.xlsx')

df9 = pd.read_excel('CSGOVNsuperout.xlsx')

df10 = pd.read_excel('ICMOVNsuperout.xlsx')

df11 = pd.read_excel('CSGOVNmgrout.xlsx')

df12 = pd.read_excel('ICMOVNmgrout.xlsx')

 

 

# concat both DataFrame into a single DataFrame

df = pd.concat([df1, df2, df3, df4, df5, df6, df7, df8, df9, df10, df11, df12])

 

# Export Dataframe into Excel file

df.to_excel('VRJan13mergedfiles.xlsx', index=False)
